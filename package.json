{
  "name": "dobruniaui-vue",
  "version": "1.2.1",
  "description": "Modern Vue 3 UI component library built with TypeScript and Vite",
  "private": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./dist/styles.css"
  ],
  "keywords": [
    "vue",
    "vue3",
    "vue 3",
    "vue component library",
    "ui library",
    "design system",
    "typescript",
    "vite",
    "storybook",
    "components",
    "ui components",
    "frontend",
    "modern ui",
    "chat ui",
    "theming",
    "dark theme",
    "a11y",
    "accessible components"
  ],
  "main": "./dist/dobruniaui.cjs",
  "module": "./dist/dobruniaui.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/dobruniaui.mjs",
      "require": "./dist/dobruniaui.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "vue": "^3.5.29"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@storybook/addon-a11y": "^10.2.13",
    "@storybook/vue3-vite": "^10.2.13",
    "storybook": "^10.2.13",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vue-tsc": "^2.0.0",
    "@storybook/addon-docs": "^10.2.13"
  }
}
